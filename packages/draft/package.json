{
  "name": "@zerologementvacant/draft",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "copy": "copyfiles -V -u 1 'src/**/*.css' 'src/**/*.hbs' 'src/**/*.json' 'src/fonts/**/*.otf' dist/lib",
    "build": "tsc --build tsconfig.lib.json && yarn copy"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      },
      "require": {
        "types": "./dist/lib/index.d.cts",
        "default": "./dist/lib/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@swc/core": "1.13.3",
    "@swc/jest": "0.2.39",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.4",
    "@tsconfig/node24": "24.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.10",
    "copyfiles": "2.4.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-extended": "4.0.2",
    "nodemon": "3.1.10",
    "ts-jest": "29.4.1"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "1.28.0",
    "@pdfme/common": "5.4.2",
    "@pdfme/generator": "5.4.2",
    "@pdfme/schemas": "5.4.2",
    "@zerologementvacant/utils": "workspace:*",
    "async": "3.2.6",
    "date-fns": "4.1.0",
    "handlebars": "4.7.8",
    "node-html-parser": "7.0.1",
    "pdf-lib": "1.17.1",
    "puppeteer": "22.15.0",
    "ts-pattern": "5.8.0",
    "unpdf": "1.1.0"
  }
}
