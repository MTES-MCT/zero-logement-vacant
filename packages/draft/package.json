{
  "name": "@zerologementvacant/draft",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index-next.js",
  "types": "dist/index-next.d.ts",
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "copy": "copyfiles -V -u 1 'src/**/*.css' 'src/**/*.hbs' dist",
    "typecheck": "tsc --noEmit",
    "build": "pkgroll -p tsconfig.build.json && yarn copy",
    "dev": "nodemon --watch src -e ts --exec 'yarn build'",
    "next": "tsx src/index-next.tsx",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@swc/core": "^1.11.8",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/vite-react": "^3.4.0",
    "@types/async": "^3.2.24",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.10",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.7",
    "pkgroll": "^2.11.2",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "1.9.16",
    "@faker-js/faker": "^8.4.1",
    "@gouvfr/dsfr": "1.11.2",
    "@react-pdf/renderer": "^4.3.0",
    "@zerologementvacant/utils": "workspace:*",
    "async": "^3.2.6",
    "handlebars": "^4.7.8",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^22.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ts-pattern": "^5.5.0",
    "tsx": "^4.19.3"
  }
}
