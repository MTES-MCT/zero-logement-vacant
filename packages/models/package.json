{
  "name": "@zerologementvacant/models",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fixtures": {
      "import": "./dist/test/fixtures.js",
      "require": "./dist/test/fixtures.js",
      "types": "./dist/test/fixtures.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "test": "jest",
    "coverage": "jest --coverage",
    "dev": "nodemon --watch src -e ts --exec 'yarn build'"
  },
  "dependencies": {
    "@zerologementvacant/draft": "workspace:*",
    "@zerologementvacant/utils": "workspace:*",
    "lodash": "^4.17.21",
    "ts-essentials": "^10.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.11.1",
    "@swc/jest": "^0.2.37",
    "@tsconfig/node20": "^20.1.4",
    "@types/geojson": "^7946.0.14",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.9",
    "@types/node": "^20.16.10",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.7",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  }
}
