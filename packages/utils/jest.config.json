{
  "transform": {
    "^.+\\.(t|j)sx?$": "@swc/jest"
  },
  "testEnvironment": "node",
  "rootDir": "src",
  "collectCoverageFrom": [
    "<rootDir>/**/*.ts",
    "!<rootDir>/**/test/*",
    "!<rootDir>/**/index.ts"
  ],
  "setupFilesAfterEnv": ["jest-extended/all", "<rootDir>/test/setup-tests.ts"]
}
